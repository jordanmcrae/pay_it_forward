<%= link_to "Back to Projects", root_url %> <% if current_user %> | <span class="label label-warning"><%= link_to "Edit", edit_project_path(@project) %></span> | <span class="label label-danger"><%= link_to "Delete", @project, method: :delete, data: {confirm: "Are you sure?"} %><% end %></span>

  <h1><%= @project.name %></h1>
  <h3><%= @project.description %></h3>
  <h3><%= @project.funding_goal %></h3>
  <h3><%= @project.start_date %></h3>
  <h3><%= @project.end_date %></h3>